
int resta(int x, int y){

	return x-y;
}

int divide(int x, int y){

	return x/y;
}




int Suma(int a, int b){
  return a+b;
}

int Multiplicar(int a, int b){
  return a*b;
}



int raiz(int a){

	int resultado2 = (float) Math.pow(a, 1/2);
	return resultado2;
}

int Potencia(int numero, int base){
			int temp=1;
      for(int i = 0; i<=base; i++){
				temp = numero*temp;
			}
			return temp;

}
