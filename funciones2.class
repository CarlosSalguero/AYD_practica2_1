int factorial(){ 

	if(n==0 || n==1) 
		return n;
	 
	for(int i=2; i<=n; i++)
		res=res*i;
	return res;

}

//no se cambio nada, funcion factorial2 200819214
int factorial(int numero){
  if ( numero <= 1 ) {
             return 1;
      } else {
             return numero*factorial(numero-1);
      }

}


